{
    "implementation_phases": {
        "phase_1": {
            "name": "Migration Pilote",
            "agents": ["agent_05", "agent_111", "agent_MAINTENANCE_00"],
            "duration_days": 7,
            "validation_criteria": {
                "matrix_success_rate": 95.0,
                "monitoring_precision": 90.0,
                "message_delivery_rate": 98.0,
                "optimization_improvement": 10.0
            }
        },
        "phase_2": {
            "name": "Migration Principale",
            "agents": ["agent_109"],
            "duration_days": 14,
            "validation_criteria": {
                "matrix_success_rate": 97.0,
                "monitoring_precision": 92.0,
                "message_delivery_rate": 99.0,
                "optimization_improvement": 12.0
            }
        }
    },
    "monitoring": {
        "check_intervals": [60, 300, 900, 3600],
        "metrics": [
            "cpu_usage",
            "memory_usage",
            "disk_io",
            "network_latency",
            "response_time",
            "error_rate",
            "throughput"
        ],
        "thresholds": {
            "cpu_usage": {
                "warning": 75,
                "critical": 90
            },
            "memory_usage": {
                "warning": 80,
                "critical": 95
            },
            "disk_io": {
                "warning": 70,
                "critical": 85
            },
            "network_latency": {
                "warning": 100,
                "critical": 200
            },
            "response_time": {
                "warning": 500,
                "critical": 1000
            },
            "error_rate": {
                "warning": 1,
                "critical": 5
            },
            "throughput": {
                "warning": 1000,
                "critical": 500
            }
        }
    },
    "logging": {
        "log_level": "DEBUG",
        "file_rotation": {
            "max_size_mb": 100,
            "backup_count": 5
        },
        "formats": {
            "detailed": "%(asctime)s [%(levelname)s] %(message)s",
            "console": "%(asctime)s - %(message)s"
        }
    },
    "reporting": {
        "frequency": "daily",
        "formats": ["json", "markdown"],
        "metrics_retention_days": 30,
        "alert_channels": ["email", "slack"]
    }
} 