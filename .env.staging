# Configuration staging Agent Factory
ENVIRONMENT=staging
LOG_LEVEL=INFO
DEBUG=false

# Configuration cache
CACHE_TTL=300
CACHE_MAX_SIZE=500

# Configuration threads
THREAD_POOL_SIZE=4

# Hot reload
HOT_RELOAD=true

# Variables scurises ( configurer)
SECRET_KEY=${STAGING_SECRET_KEY}
ENCRYPTION_KEY=${STAGING_ENCRYPTION_KEY}

# Monitoring
PROMETHEUS_PORT=8080
METRICS_ENABLED=true
